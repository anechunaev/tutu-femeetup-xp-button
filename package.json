{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Frontend Boilerplate",
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/bungu/frontend-boilerplate"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack --mode=production --config=config/webpack/client.prod.js",
    "build:server": "webpack --mode=production --config=config/webpack/server.prod.js",
    "build": "npm run build:client & npm run build:server",
    "dev:build:client": "webpack --mode=development --config=config/webpack/client.dev.js",
    "dev:build:server": "webpack --mode=development --config=config/webpack/server.dev.js",
    "dev:build": "npm run dev:build:client & npm run dev:build:server",
    "start": "node dist/server.js",
    "watch": "nodemon -x 'bash watch.sh'",
    "yo": "yo",
    "yo:j": "yo order:rj",
    "yo:jm": "yo order:rjm",
    "yo:l": "yo order:rl",
    "yo:lm": "yo order:rlm",
    "yo:lt": "yo order:rlt"
  },
  "devDependencies": {
    "@types/csso": "^3.5.0",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "awesome-typescript-loader": "^5.2.0",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "generator-order": "file:generators",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "style-loader": "^0.22.1",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-manifest-plugin": "^2.0.3",
    "yeoman-generator": "^3.1.1",
    "yo": "^2.0.5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "csso": "^3.5.1",
    "express": "^4.16.3",
    "jss": "^9.8.7",
    "react": "^16.4.2",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.4.2",
    "react-jss": "^8.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "themr": "0.1.0-alpha.2"
  },
  "main": "entry/server.tsx",
  "directories": {
    "lib": "lib"
  },
  "author": "Artem Nechunaev <nechunaev@tutu.ru>",
  "sideEffects": false
}
